package com.sample
 
import java.util.ArrayList;

declare Question
	question: String
	respwers: ArrayList
	result: String
end

rule "Init"
	when
	then
		ArrayList<String> options = new ArrayList();
		options.add("Yes");
		options.add("No");
		String question = "Are you in Mexico?(or in a Mexican restaurant)";
		GuiClass p1 = new GuiClass(question, options);
		String resp = p1.gui();
		insert(new Question(question, options, resp));
end;

rule "Are you in Mexico - Yes"
    when
        exists Question(question == "Are you in Mexico?(or in a Mexican restaurant)", result == "Yes")
    then
        String[] pictures = {"1"};
        new GuiClass("", new ArrayList<String>()).showResult(pictures); 
end;
